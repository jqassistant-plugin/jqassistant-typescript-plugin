{
  "compilerOptions": {
    "target": "ESNext",
    "experimentalDecorators": true,
    "module": "Node16",
    "moduleResolution": "node16",
    "resolveJsonModule": true,
    "allowJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitOverride": true,
    "skipLibCheck": true,
    "isolatedModules": true
  }
}
